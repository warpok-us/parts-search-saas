{
  "name": "@partsy/types",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm",
    "dev": "tsup src/index.ts --watch --dts --format esm",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@partsy/tsconfig": "workspace:*",
    "typescript": "5.8.2",
    "tsup": "^8.0.0"
  }
}
